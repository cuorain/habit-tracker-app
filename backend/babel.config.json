{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-syntax-import-attributes",
      { "deprecatedAssertSyntax": true }
    ],
    "babel-plugin-transform-import-meta"
  ]
}
